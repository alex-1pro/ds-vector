pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
            	echo 'build'
                sh 'make lib'
            }
        }
        stage('Clear') { 
            steps {
            	echo 'Clear'
                sh 'make clear'
            }
        }
        stage('Deploy') { 
            steps {
                echo 'Deploy' 
                sh 'curl -H "X-JFrog-Art-Api:AKCp8kr1CcVd5rfGm5rwLsdZidDktdUAba8DqG6ikxfuBZEBua7yReY8UkYSEC3PmUKYgF7qt" -T  "https://artifactoyexe.jfrog.io/artifactory/jenkins-project/dslib.a" '
            }
        }
    }
}
